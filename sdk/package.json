{
  "name": "jourycms-sdk",
  "version": "1.1.17",
  "description": "Sdk for Joury CMS",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "typings/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reevosolutions/jourycms-project.git"
  },
  "keywords": [
    "levelup",
    "sdk"
  ],
  "author": "salmigroup@gmail.com",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "lint:src": "eslint 'src/**/*.ts'",
    "lint:typing": "eslint 'typings/**/*.d.ts'",
    "lint": "yarn lint:src && yarn lint:typing",
    "lint:fix": "eslint ./src --fix",
    "push": "git add . && git commit -m 'update' && git push",
    "pub": "npm run build && npm run push && npm publish",
    "up": "npm version patch && npm publish"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "colors": "^1.4.0",
    "moment": "^2.30.1",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.96",
    "@types/qs": "^6.9.16",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "utility-types": "^3.11.0"
  }
}
